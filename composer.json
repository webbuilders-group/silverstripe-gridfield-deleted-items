{
    "name": "webbuilders-group/silverstripe-gridfield-deleted-items",
    "description": "Provides a series of components that allows you to view versioned objects that have been deleted from the draft site.",
    "type": "silverstripe-vendormodule",
    "keywords": ["silverstripe", "gridfield", "versioned", "deleted"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Ed Chipman",
            "homepage": "http://webbuildersgroup.com",
            "role": "Developer"
        }
    ],
    "require": {
        "silverstripe/framework": "^6",
        "silverstripe/versioned": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~11.3"
    },
    "support": {
        "issues": "https://github.com/webbuilders-group/silverstripe-gridfield-deleted-items/issues"
    },
	"extra": {
        "expose": [
            "css"
        ]
	},
    "autoload": {
        "psr-4": {
            "WebbuildersGroup\\GridFieldDeletedItems\\": "src/",
            "WebbuildersGroup\\GridFieldDeletedItems\\Tests\\": "tests/"
        }
    }
}
